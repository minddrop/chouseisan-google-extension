{
	"manifest_version": 2,
	"name": "調整さん入力支援",
	"version": "4.5.67.890",
	"description": "調整さんごにょごにょしてくれます",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_popup": "popup/index.html"
	},
	"permissions": [
		"https://chouseisan.com/",
		"tabs"
	],
	"content_scripts": [
		{
			"matches": [
				"https://chouseisan.com/*"
			],
			"js": [
				"script.js"
			]
		}
	]
}
