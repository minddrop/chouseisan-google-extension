{
	"manifest_version": 2,
	"name": "調整さん入力支援",
	"version": "4.5.6.7",
	"description": "調整さんごにょごにょしてくれます",
	"icons": {},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"https://chouseisan.com/",
		"tabs"
	],
	"content_scripts": [
		{
			"matches": [
				"https://chouseisan.com/*"
			],
			"js": [
				"script.js"
			]
		}
	]
}